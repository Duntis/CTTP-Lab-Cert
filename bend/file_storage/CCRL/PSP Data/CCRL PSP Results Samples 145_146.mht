var menuTimeout =3D 400

var menuSections =3D new Array()
var menuCountHide =3D new Array()

var menuSectionCnt =3D 0
var menuBoxCnt =3D 0

function menuShow(section, elements) {
  for (var i =3D 0; i < menuSections.length; i++) {
    if (menuSections[i] !=3D section) {
      menuHide(menuSections[i], menuCountNodes(menuSections[i]))
    }
  }
  for (var i =3D 1; i <=3D elements; i++) {
    document.getElementById(section + '-' + i).style.visibility =3D =
'visible'
  }
}

function menuHide(section, elements) {
  for (var i =3D 1; i <=3D elements; i++) {
    document.getElementById(section + '-' + i).style.visibility =3D =
'hidden'
  }
  document.getElementById(section).style.zIndex =3D -1
}

function menuTryHide(section, elements, countHide) {
  if (countHide !=3D menuCountHide[section]) {
    return
  }
  menuHide(section, elements)
}

function menuCountNodes(element) {
  ret =3D 0
  nodes =3D document.getElementById(element).childNodes.length
  for (var i =3D 0; i < nodes; i++) {
    if (document.getElementById(element).childNodes[i].nodeType =3D=3D =
1) {
      ret++
    }
  }
  return ret
}

function menuInitSection(section) {
  var elements =3D menuCountNodes(section)
  for (var i =3D 0; i <=3D elements; i++) {
    var s =3D (i =3D=3D 0 ? (section + '-top') : (section + '-' + i))
    if (i =3D=3D 0) {
      document.getElementById(s).onmouseover =3D function() {
        menuShow(section, elements)
        menuCountHide[section]++
        for (var ii =3D 0; ii < menuSections.length; ii++) {
          document.getElementById(section).style.zIndex =3D 1
          if (menuSections[ii] !=3D section) {
            document.getElementById(menuSections[ii]).style.zIndex =3D =
-1
          }
        }
      }
    } else {
      document.getElementById(s).onmouseover =3D function() {
        //menuShow(section, elements)
        menuCountHide[section]++
      }
    }
    document.getElementById(s).onmouseout =3D function() {
      setTimeout("menuTryHide('" + section + "', " + elements + ", " + =
menuCountHide[section] + ")", menuTimeout)
    }
  }
}

function menuMakeId(nodes) {
  for (var i =3D 0; i < nodes.length; i++) {
    switch (nodes[i].className) {
      case 'top':
        menuSectionCnt++
        menuBoxCnt =3D 0
        nodes[i].id =3D 'menu-' + menuSectionCnt + '-top'
        break
      case 'section':
        nodes[i].id =3D 'menu-' + menuSectionCnt
        menuSections[menuSections.length] =3D nodes[i].id
        break
      case 'box':
        menuBoxCnt++
        nodes[i].id =3D 'menu-' + menuSectionCnt + '-' + menuBoxCnt
        break
    }
    if (nodes[i].childNodes) {
      menuMakeId(nodes[i].childNodes)
    }
  }
}

function menuInit() {
  menuMakeId(document.getElementById('menu').childNodes)
  for (var i =3D 0; i < menuSections.length; i++) {
    menuCountHide[menuSections[i]] =3D 0
  }
  for (var i =3D 0; i < menuSections.length; i++) {
    menuInitSection(menuSections[i])
  }
}
